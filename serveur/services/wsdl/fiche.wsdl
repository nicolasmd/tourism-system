	</message>

        <message name="getFichesRequest">
            <part name="sessionId" type="xsd:string"/>
        </message>
        <message name="getFichesResponse">
            <part name="status" type="tns:WSstatus"/>
            <part name="fiches" nillable="true" type="xs:anyType"/>

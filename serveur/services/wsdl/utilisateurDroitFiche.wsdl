				</all>
			</complexType>
			
			
			<complexType name="ArrayOfDroitsTerritoire">
				<complexContent mixed="false">
					<restriction base="soapenc:Array">
						<attribute ref="soapenc:arrayType" wsdl:arrayType="DroitTerritoire[]"/>
					</restriction>
				</complexContent>
			</complexType>
			<complexType name="DroitTerritoire">

					<element name="suppressionFiches" nillable="true" type="xsd:boolean"/>
					<element name="creationFiches" nillable="true" type="xsd:boolean"/>
					<element name="administration" nillable="true" type="xsd:boolean"/>
				</all>
			</complexType>

			
		</xsd:schema>
	</wsdl:types>
	
	
	
	
